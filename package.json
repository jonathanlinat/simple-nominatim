{
  "name": "simple-nominatim",
  "description": "Monorepo for Simple Nominatim packages",
  "version": "0.0.0",
  "author": "Jonathan Linat (https://github.com/jonathanlinat)",
  "bugs": "https://github.com/jonathanlinat/simple-nominatim/issues",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/format": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/eslint-plugin": "^1.4.0",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-boundaries": "^5.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-markdownlint": "^0.9.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-yml": "^1.19.0",
    "execa": "^9.6.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.1",
    "syncpack": "^13.0.4",
    "tsdown": "^0.16.0",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.7"
  },
  "engines": {
    "bun": "please-use-pnpm",
    "deno": "please-use-pnpm",
    "node": "22.17.1",
    "npm": "please-use-pnpm",
    "pnpm": "10.18.3",
    "yarn": "please-use-pnpm"
  },
  "homepage": "https://github.com/jonathanlinat/simple-nominatim#readme",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "private": true,
  "repository": "jonathanlinat/simple-nominatim.git",
  "scripts": {
    "ci:release": "changeset publish",
    "ci:version": "changeset version",
    "clean": "turbo run clean && rm -rf node_modules",
    "deps:check": "syncpack lint",
    "deps:fix": "syncpack fix-mismatches",
    "deps:format": "syncpack format",
    "deps:list": "syncpack list",
    "deps:update": "syncpack update",
    "lint": "eslint . --fix",
    "packages:build": "turbo run build",
    "packages:build:watch": "turbo run build --watch",
    "postinstall": "pnpm simple-git-hooks",
    "prepublishOnly": "pnpm packages:build && pnpm test",
    "security:audit": "pnpm audit --audit-level=moderate",
    "security:audit:fix": "pnpm audit --fix",
    "test": "vitest run",
    "test:e2e": "vitest run packages/*/src/__tests__/**/*.e2e.test.ts",
    "test:types": "vitest run packages/*/src/__tests__/**/*.types.test.ts",
    "test:unit": "vitest run packages/*/src/__tests__/**/*.unit.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "type": "module"
}
