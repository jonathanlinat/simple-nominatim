{
  "name": "@simple-nominatim/core",
  "version": "0.3.0",
  "description": "A lightweight library to make requests to the Nominatim API from Node programs",
  "keywords": [
    "core",
    "geocoding",
    "library",
    "node",
    "nominatim",
    "openstreetmap"
  ],
  "homepage": "https://github.com/jonathanlinat/simple-nominatim/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/jonathanlinat/simple-nominatim/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanlinat/simple-nominatim.git"
  },
  "license": "MIT",
  "author": "Jonathan Linat (https://github.com/jonathanlinat)",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "prepare": "pnpm build",
    "test": "vitest run --typecheck",
    "test:coverage": "vitest run --typecheck --coverage",
    "test:ui": "vitest --typecheck --ui",
    "test:watch": "vitest --typecheck",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "lru-cache": "^11.2.2",
    "p-throttle": "^8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.17.1"
  }
}
