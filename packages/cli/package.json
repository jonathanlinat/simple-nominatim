{
  "name": "@simple-nominatim/cli",
  "description": "A command-line interface (CLI) tool built on top of the @simple-nominatim/core library to make requests to the Nominatim API from a terminal",
  "version": "0.6.2",
  "author": "Jonathan Linat (https://github.com/jonathanlinat)",
  "bin": {
    "simple-nominatim": "./dist/index.js"
  },
  "bugs": "https://github.com/jonathanlinat/simple-nominatim/issues",
  "dependencies": {
    "@simple-nominatim/core": "workspace:*",
    "commander": "^14.0.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.17.1"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/jonathanlinat/simple-nominatim/tree/main/packages/cli#readme",
  "keywords": [
    "cli",
    "geocoding",
    "library",
    "node",
    "nominatim",
    "openstreetmap"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "jonathanlinat/simple-nominatim.git",
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "prepare": "pnpm build",
    "simple-nominatim": "tsx ./src/index.ts"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
